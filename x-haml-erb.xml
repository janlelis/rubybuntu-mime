<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="text/x-haml-erb">
    <sub-class-of type="text/x-haml"/>
    <comment>Ruby embedded in Haml (erb)</comment>
    <glob pattern="*.haml.erb"/>
    <magic priority="50">
      <match value="&lt;%" type="string" offset="0"/>
    </magic>
  </mime-type>
</mime-info>
